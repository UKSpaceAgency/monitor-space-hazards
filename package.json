{
  "name": "monitorspacehazards",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev:spotlight": "pnpx @spotlightjs/spotlight",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "commit": "cz",
    "prepare": "husky",
    "generate-api": "vite-node generateApi.ts",
    "push-i18n": "crowdin upload sources",
    "pull-i18n": "crowdin download sources"
  },
